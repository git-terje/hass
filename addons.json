{
  "pos": {
    "name": "POS",
    "version": "0.2.9",
    "slug": "ha_pos",
    "description": "Point of Sales system for Home Assistant using Google Sheets backend (with ingress, secrets-based, all platforms)",
    "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
    "startup": "services",
    "boot": "auto",
    "ingress": true,
    "ingress_port": 8091,
    "panel_icon": "mdi:cash-register",
    "panel_title": "HA POS",
    "options": {
      "credentials_file": "!secret pos_service_account_path",
      "sheet_id": "!secret pos_sheet_id",
      "worksheet": "!secret pos_sheet_name"
    },
    "schema": {
      "credentials_file": "str",
      "sheet_id": "str",
      "worksheet": "str"
    },
    "hassio_api": true,
    "url": "https://github.com/git-terje/hass",
    "maintainer": "git-terje",
    "stage": "experimental"
  },
  "pos_pot": {
    "name": "POS Pot",
    "version": "0.1.0",
    "slug": "pos_pot",
    "description": "Point of Sale system for cannabis sales integrated with Home Assistant (MariaDB backend).",
    "arch": ["aarch64", "amd64", "armv7"],
    "startup": "services",
    "boot": "auto",
    "ports": {
      "8180/tcp": 8180
    },
    "panel_icon": "mdi:cash-register",
    "panel_title": "POS Pot",
    "options": {
      "db_url": "mysql+pymysql://homeassistant:deploy@core-mariadb/homeassistant"
    },
    "schema": {
      "db_url": "str"
    },
    "url": "https://github.com/git-terje/hass",
    "maintainer": "git-terje",
    "stage": "experimental"
  },
  "ha_sysinfo": {
    "name": "Home Assistant System Info",
    "version": "1.0.7",
    "slug": "ha_sysinfo",
    "description": "Collects and exposes system info, addons, integrations and filtered logs.",
    "arch": ["aarch64", "amd64", "armv7", "i386"],
    "startup": "application",
    "boot": "auto",
    "hassio_api": true,
    "homeassistant_api": true,
    "auth_api": true,
    "host_network": true,
    "panel_icon": "mdi:information",
    "panel_title": "HA Sysinfo",
    "image": "ghcr.io/git-terje/ha_sysinfo"
  }
}
