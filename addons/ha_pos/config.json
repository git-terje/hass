{
  "name": "HA Point of Sales",
  "slug": "ha_pos",
  "version": "0.2.2",
  "description": "Point of Sales system for Home Assistant using Google Sheets backend (with ingress, secrets-based, all platforms)",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "startup": "services",
  "boot": "auto",
  "ingress": true,
  "ingress_port": 8091,
  "panel_icon": "mdi:cash-register",
  "panel_title": "HA POS",
  "options": {
    "credentials_file": "!secret google_creds",
    "sheet_id": "!secret google_sheet_id",
    "worksheet": "!secret google_sheet_ws"
  },
  "schema": {
    "credentials_file": "str",
    "sheet_id": "str",
    "worksheet": "str"
  },
  "url": "https://github.com/git-terje/hass",
  "maintainer": "git-terje",
  "stage": "experimental"
}
