{
  "name": "POS Pot",
  "version": "0.1.7",
  "slug": "pos_pot",
  "description": "Point of Sale system for cannabis sales integrated with Home Assistant (MariaDB backend).",
  "arch": ["aarch64", "amd64", "armv7", "armhf", "i386"],
  "startup": "services",
  "boot": "auto",
  "ports": {
    "8180/tcp": 8180
  },
  "map": ["config:rw", "ssl:rw", "share:rw"],
  "options": {
    "db_url": "mysql+pymysql://homeassistant:deploy@core-mariadb/homeassistant"
  },
  "schema": {
    "db_url": "str"
  },
  "url": "https://github.com/git-terje/hass",
  "maintainer": "git-terje",
  "stage": "experimental"
}
